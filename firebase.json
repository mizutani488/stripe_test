{
    "database": {
        "rules": "database.rules.json"
    },
    "firestore": {
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    },
    "functions": {},
    "hosting": {
        "public": "public",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ]
    },
    "storage": {
        "rules": "storage.rules"
    },
    "emulators": {
        "functions": {
            "port": 5001
        },
        "ui": {
            "enabled": true
        }
    },
    "remoteconfig": {
        "template": "remoteconfig.template.json"
    }
}